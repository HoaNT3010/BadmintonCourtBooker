{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "logs/log-.txt",
          "textFormatter": "JsonFormatter",
          "rollingInterval": "Day",
          "rollOnFileSizeLimit": true,
          "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;uid=sa;pwd=12345;database=BadmintonCourtBooker;TrustServerCertificate=True"
  },
  "Jwt": {
    "Issuer": "BadmintonCourtBooker_Issuer",
    "Audience": "BadmintonCourtBooker_Audience",
    "SecretKey": "GQF89Nf7WSWHSdXJ/TLFWe3eLSChdTTZFpO2I1Lg8Y8"
  }
}
